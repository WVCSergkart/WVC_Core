<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="MechanoidBuildings_WVC" Name="WVC_MechanoidIncubator_Base">
	<defName>WVC_IncubatorLF</defName>
	<!-- <label>incubator-CT</label> -->
	<!-- <description>Complex structure for cat incubation. The process of creating cats requires an enormous amount of resources, from a special chemical liquid to certain crystalline formations. Luckily, the Incubator-CT complex is fully automated and requires no power, workers, or maintenance, but it can only be placed underground.\n\n&lt;color=#f5ffa2&gt;Gameplay Effect:&lt;/color&gt; Without the mod "kemomimilike" will spawn cats every few days.</description> -->
	<label>incubator-LF</label>
	<description>Complex structure for ELF incubation. The process of creating semi-organic helpers requires an enormous amount of resources, from a special chemical liquid to certain crystalline formations. Luckily, the Incubator-LF complex is fully automated and requires no power, workers, or maintenance, but it can only be placed underground.\n\n&lt;color=#f5ffa2&gt;Note:&lt;/color&gt; Without the mods "WVC - kemomimilike" + "Vanilla Ideology Expanded - Dryads" does nothing and is a decorative element.\n\n&lt;color=#f5ffa2&gt;Note2:&lt;/color&gt; With the "WVC - Nodes" mod, the incubator is a constant source of nodes.</description>
	<descriptionHyperlinks>
	  <ConceptDef>WVC_AboutIncubatorLF</ConceptDef>
	</descriptionHyperlinks>
	<thingClass>Building</thingClass>
	<drawerType>MapMeshAndRealTime</drawerType>
	<graphicData>
	  <texPath>WVC/Stuff/Incubator</texPath>
	  <graphicClass>Graphic_Random</graphicClass>
	  <drawSize>(6,6)</drawSize>
	  <shadowData>
		<volume>(6.03,1.0,5.25)</volume>
		<offset>(0,0,-0.25)</offset>
	  </shadowData>
	  <damageData>
		<rect>(0.3,0,5.4,6)</rect>
	  </damageData>
	</graphicData>
	<altitudeLayer>Building</altitudeLayer>
	<passability>Impassable</passability>
	<blockWind>true</blockWind>
	<scatterableOnMapGen>false</scatterableOnMapGen>
	<fillPercent>1</fillPercent>
	<blockLight>true</blockLight>
	<canOverlapZones>false</canOverlapZones>
	<holdsRoof>true</holdsRoof>
	<statBases>
	  <!-- <MarketValue>1</MarketValue> -->
	  <MaxHitPoints>5000</MaxHitPoints>
	  <WorkToBuild>50000</WorkToBuild>
	  <Flammability>0.0</Flammability>
	  <Beauty>-30</Beauty>
	</statBases>
	<tickerType>Normal</tickerType>
	<size>(6,6)</size>
	<costList>
	  <Plasteel>1250</Plasteel>
	  <Uranium>333</Uranium>
	  <ComponentSpacer>12</ComponentSpacer>
	  <VFE_ComponentMechanoid>12</VFE_ComponentMechanoid>
	  <VFE_ComponentMechanoid>15</VFE_ComponentMechanoid>
	</costList>
	<killedLeavings>
	  <ChunkSlagSteel>33</ChunkSlagSteel>
	</killedLeavings>
	<comps>
	  <li Class="CompProperties_Flickable"/>
	  <li Class="CompProperties_Power">
		<compClass>CompPowerTrader</compClass>
		<basePowerConsumption>0</basePowerConsumption>
		<transmitsPower>true</transmitsPower>
	  </li>
	  <li MayRequire="honkai.girls.sergkart" Class="CompProperties_Spawner" WVC_Tag="RemoveItFromIncubator">
		<requiresPower>true</requiresPower>
		<thingToSpawn>WVC_CapsuleILF</thingToSpawn>
		<spawnCount>1</spawnCount>
		<spawnIntervalRange><min>300000</min><max>900000</max></spawnIntervalRange>
		<writeTimeLeftToSpawn>true</writeTimeLeftToSpawn>
		<showMessageIfOwned>false</showMessageIfOwned>
		<!-- <inheritFaction>true</inheritFaction> -->
	  </li>
	  <!-- <li MayRequire="wvc.sergkart.nodes" Class="WVC.CompProperties_SpawnerItems"> -->
		<!-- <approxMarketValuePerDay>5000</approxMarketValuePerDay> -->
		<!-- <categories> -->
		  <!-- <li>WVC_ResourceSubNodes</li> -->
		<!-- </categories> -->
		<!-- <spawnInterval>300000</spawnInterval> -->
	  <!-- </li> -->
	  <li MayRequire="wvc.sergkart.nodes" Class="CompProperties_Spawner">
		<requiresPower>true</requiresPower>
		<thingToSpawn>WVC_ResourceNodes_Blue</thingToSpawn>
		<spawnCount>18</spawnCount>
		<spawnIntervalRange><min>450000</min><max>600000</max></spawnIntervalRange>
		<writeTimeLeftToSpawn>false</writeTimeLeftToSpawn>
		<showMessageIfOwned>false</showMessageIfOwned>
	  </li>
	  <li MayRequire="wvc.sergkart.nodes" Class="CompProperties_Spawner">
		<requiresPower>true</requiresPower>
		<thingToSpawn>WVC_ResourceNodes_Green</thingToSpawn>
		<spawnCount>19</spawnCount>
		<spawnIntervalRange><min>450000</min><max>600000</max></spawnIntervalRange>
		<writeTimeLeftToSpawn>false</writeTimeLeftToSpawn>
		<showMessageIfOwned>false</showMessageIfOwned>
	  </li>
	  <li MayRequire="wvc.sergkart.nodes" Class="CompProperties_Spawner">
		<requiresPower>true</requiresPower>
		<thingToSpawn>WVC_ResourceNodes_Red</thingToSpawn>
		<spawnCount>22</spawnCount>
		<spawnIntervalRange><min>450000</min><max>600000</max></spawnIntervalRange>
		<writeTimeLeftToSpawn>false</writeTimeLeftToSpawn>
		<showMessageIfOwned>false</showMessageIfOwned>
	  </li>
	  <li MayRequire="wvc.sergkart.nodes" Class="CompProperties_Spawner">
		<requiresPower>true</requiresPower>
		<thingToSpawn>WVC_ResourceNodes_Violet</thingToSpawn>
		<spawnCount>20</spawnCount>
		<spawnIntervalRange><min>450000</min><max>600000</max></spawnIntervalRange>
		<writeTimeLeftToSpawn>false</writeTimeLeftToSpawn>
		<showMessageIfOwned>false</showMessageIfOwned>
	  </li>
	  <li MayRequire="wvc.sergkart.nodes" Class="CompProperties_Spawner">
		<requiresPower>true</requiresPower>
		<thingToSpawn>WVC_ResourceNodes_Yellow</thingToSpawn>
		<spawnCount>21</spawnCount>
		<spawnIntervalRange><min>450000</min><max>600000</max></spawnIntervalRange>
		<writeTimeLeftToSpawn>false</writeTimeLeftToSpawn>
		<showMessageIfOwned>false</showMessageIfOwned>
	  </li>
	</comps>
	<rotatable>false</rotatable>
	<terrainAffordanceNeeded>MechanoidUnderground_WVC</terrainAffordanceNeeded>
	<designationCategory>Misc</designationCategory>
	<placeWorkers>
	  <!-- <li>Placeworker_OnlyUnderThickRoof</li> -->
	  <!-- <li>PlaceWorker_OnSteamGeyser</li> -->
	</placeWorkers>
	<building>
	  <ai_chillDestination>false</ai_chillDestination>
	  <destroySound>BuildingDestroyed_Metal_Big</destroySound>
	</building>
	<constructEffect>ConstructMetal</constructEffect>
	<researchPrerequisites Inherit="False">
		<li>WVC_MechanoidTech_IncubatorLF</li>
	</researchPrerequisites>
	<constructionSkillPrerequisite>16</constructionSkillPrerequisite>
  </ThingDef>

