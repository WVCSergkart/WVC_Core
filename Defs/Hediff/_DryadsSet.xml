  <HediffDef>
	<defName>Extra_VenerableDryads_WVC</defName>
	<hediffClass>HediffWithComps</hediffClass>
	<label>Venerable dryad</label>
	<description>This dryad lives long enough to be considered respectable, is stronger than young dryads and knows how to survive.</description>
	<defaultLabelColor>(255,150,100)</defaultLabelColor>
	<everCurableByItem>false</everCurableByItem>
	<scenarioCanAdd>false</scenarioCanAdd>
	<isBad>false</isBad>
	<makesAlert>false</makesAlert>
	<comps>
		<!-- <li Class="WVC.HediffCompProperties_Healing" > -->
			<!-- <tickBeforeHeal>897500</tickBeforeHeal> -->
		<!-- </li> -->
		<!-- <li Class="HediffCompProperties_HealPermanentWounds" /> -->
		<li Class="WVC.HediffCompProperties_HealBodyParts" />
	</comps>
	<stages>
	  <li>
		<!-- <becomeVisible>false</becomeVisible> -->
		<totalBleedFactor>0.7</totalBleedFactor>
		<!-- <naturalHealingFactor>1.2</naturalHealingFactor> -->
		<statFactors>
			<InjuryHealingFactor>1.2</InjuryHealingFactor>
			<IncomingDamageFactor>0.70</IncomingDamageFactor>
			<ImmunityGainSpeed>1.5</ImmunityGainSpeed>
			<PawnTrapSpringChance>0.0</PawnTrapSpringChance>
			<FilthRate>0.0</FilthRate>
		</statFactors>
		<statOffsets>
			<ArmorRating_Blunt>0.50</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.50</ArmorRating_Sharp>
			<!-- <ArmorRating_Heat>0.30</ArmorRating_Heat> -->
		</statOffsets>
		<capMods>
		  <li>
			<capacity>Consciousness</capacity>
			<offset>0.1</offset>
		  </li>
		  <li>
			<capacity>Moving</capacity>
			<offset>0.2</offset>
		  </li>
		  <li>
			<capacity>Manipulation</capacity>
			<offset>0.1</offset>
		  </li>
		</capMods>
	  </li>
	</stages>
  </HediffDef>
  
