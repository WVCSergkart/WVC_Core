<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!--=====================================================================-->

	<Operation Class="XmlExtensions.FindMod">
		<mods>
			<li>frozensnowfox.advancedbionicsexpansion</li>
		</mods>
		<packageId>true</packageId>
		<caseTrue>

			<Operation Class="PatchOperationReplace">
			  <xpath>/Defs/HediffDef[@Name="WVC_Ultra_ImplantHediffDef_WVC_UltratechBionic"]/addedPartProps/partEfficiency</xpath>
			  <value>
				  <partEfficiency>1.4</partEfficiency>
			  </value>
			</Operation>
			<Operation Class="PatchOperationReplace">
			  <xpath>/Defs/HediffDef[@Name="WVC_Ultra_ImplantHediffDef_WVC_MechtechBionic"]/addedPartProps/partEfficiency</xpath>
			  <value>
				  <partEfficiency>1.6</partEfficiency>
			  </value>
			</Operation>
			<Operation Class="PatchOperationReplace">
			  <xpath>/Defs/HediffDef[@Name="WVC_Ultra_ImplantHediffDef_WVC_RecoveryMechtechBionic" or @Name="WVC_Ultra_ImplantHediffDef_WVC_PsychicMechtechBionic" or @Name="WVC_Ultra_ImplantHediffDef_WVC_ArmoredMechtechBionic" or @Name="WVC_Ultra_ImplantHediffDef_WVC_ToughMechtechBionic"]/addedPartProps/partEfficiency</xpath>
			  <value>
				  <partEfficiency>1.75</partEfficiency>
			  </value>
			</Operation>
			<Operation Class="PatchOperationReplace">
			  <xpath>/Defs/HediffDef[@Name="WVC_Ultra_ImplantHediffDef_WVC_OverclockedMechtechBionic"]/addedPartProps/partEfficiency</xpath>
			  <value>
				  <partEfficiency>1.9</partEfficiency>
			  </value>
			</Operation>
			<Operation Class="PatchOperationReplace">
			  <xpath>/Defs/HediffDef[@Name="WVC_Ultra_ImplantHediffDef_WVC_OverclockedMechtechBionic"]/addedPartProps/partEfficiency</xpath>
			  <value>
				  <partEfficiency>2.0</partEfficiency>
			  </value>
			</Operation>

		</caseTrue>
	</Operation>

	<!--=====================================================================-->


</Patch>