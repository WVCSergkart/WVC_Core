  <ThingDef ParentName="WVC_Ultra_ResourceAlloys_Base">
	<defName>WVC_Ultrasteel</defName>
	<label>ultsteel</label>
	<description>Advanced ultra tech structural material. Ultsteel is not very strong, but is very flexible due to its molecular structure. Ideal for electronics.</description>
	<statBases>
	  <MarketValue>12</MarketValue>
	  <StuffPower_Armor_Sharp>1.04</StuffPower_Armor_Sharp>
	  <StuffPower_Armor_Blunt>0.65</StuffPower_Armor_Blunt>
	  <StuffPower_Armor_Heat>0.65</StuffPower_Armor_Heat>
	  <StuffPower_Insulation_Cold>3</StuffPower_Insulation_Cold>
	  <StuffPower_Insulation_Heat>0</StuffPower_Insulation_Heat>
	  <SharpDamageMultiplier>0.9</SharpDamageMultiplier>
	  <BluntDamageMultiplier>1.1</BluntDamageMultiplier>
	</statBases>
	<graphicData>
	  <texPath>WVC/Stuff/Resources/Ultrasteel</texPath>
	</graphicData>
	<stuffProps>
	  <categories>
		<li>Metallic</li>
	  </categories>
	  <appearance>Metal</appearance>
	  <commonality>0.001</commonality>
	  <constructEffect>ConstructMetal</constructEffect>
	  <color>(150,157,178)</color>
	  <soundImpactStuff>BulletImpact_Metal</soundImpactStuff>
	  <soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
	  <soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
	  <statFactors>
		<MaxHitPoints>1.2</MaxHitPoints>
		<Beauty>2</Beauty>
		<Flammability>0</Flammability>
		<WorkToMake>2.5</WorkToMake>
		<WorkToBuild>2.5</WorkToBuild>
		<MeleeWeapon_CooldownMultiplier>0.8</MeleeWeapon_CooldownMultiplier>
	  </statFactors>
	</stuffProps>
  </ThingDef>

