<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<XmlExtensions.SettingsMenuDef>
		<defName>Settings_Buildings_WVC</defName>
		<modId>honkai.girls.sergkart</modId>
		<settings>
			<li Class="XmlExtensions.Setting.Gap"/>
			<li Class="XmlExtensions.Setting.GapLine" />
			<!-- <li Class="XmlExtensions.Setting.Text"> -->
				<!-- <text>In this section, you can enable/disable some buildings separately.</text> -->
				<!-- <tKey>HGS_Buildings_MainDesc_WVC</tKey> -->
			<!-- </li> -->
			<!-- <li Class="XmlExtensions.Setting.Checkbox"> -->
				<!-- <key>removeCardboardStuff_wvc</key> -->
				<!-- <label>Disable all buildings</label> -->
				<!-- <defaultValue>false</defaultValue> -->
				<!-- <tKey>HGS_Buildings_removeCardboardStuff_WVC</tKey> -->
			<!-- </li> -->
			<li Class="XmlExtensions.Setting.Checkbox">
				<key>Buildings_AncientsFromRubbish_Module_WVC</key>
				<label>Enable ancient buildings (VFEA)</label>
				<tooltip>Allows you to build ancient structures from the mod "Vanilla Factions Expanded - Ancients"\nAlso makes some changes to the logic of buildings.\n\nAll buildings require "Multi-analyzer" and "Ancient techs" technologies. The computer table allows you to conduct research. Some buildings require additional researchs.\n\n(Conflicts with all mods that significantly change the buildings of the ancients)</tooltip>
				<defaultValue>true</defaultValue>
				<tKey>HGS_Buildings_AncientsFromRubbish_WVC</tKey>
				<tKeyTip>HGS_Buildings_AncientsFromRubbish_Desc_WVC</tKeyTip>
			</li>

			<!-- =================================================== -->

			<li Class="XmlExtensions.Setting.GapLine"/>
			<li Class="XmlExtensions.Setting.SplitColumns">
				<!-- <gapSize>5</gapSize> -->
				<drawLine>true</drawLine>
				<splits>
					<li>0.1</li>
					<li>0.9</li>
				</splits>
				<settings>
					<li>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/WallsDoorsColumns/AncientWall_Icon</texPath>
							<scale>0.5</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/WallsDoorsColumns/ArchotechWallIcon</texPath>
							<scale>1.0</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/WallsDoorsColumns/MechanoidIconB</texPath>
							<scale>1.0</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/MechaniteCrafter/MechaniteCrafter_north</texPath>
							<scale>0.25</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/ComponentAssembler/ComponentAssembler</texPath>
							<scale>0.25</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/Gaurler/GaurlerA</texPath>
							<scale>0.25</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/Incubator/Incubator</texPath>
							<scale>0.0625</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/Gloomlight</texPath>
							<scale>0.25</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/Heater/HeaterA</texPath>
							<scale>0.5</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/StablePowerCell</texPath>
							<scale>0.25</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/MendingStation_north</texPath>
							<scale>0.25</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/SleepPod/SleepPod_north</texPath>
							<scale>0.25</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/WeaponBench/WeaponBench</texPath>
							<scale>0.25</scale>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.DrawImage">
							<texPath>WVC/Stuff/WallsDoorsColumns/Conduit_Icon</texPath>
							<scale>0.25</scale>
						</li>
					</li>
					<li>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Ancient - Endgame structures, cheaper than mechanoid and archotech buildings but heavy and require the right surface.\n\nThe style is inspired by the shelters from the VFEA mod.</text>
									<tKey>HGS_Buildings_Ancient_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Archotech - Endgame structures, walls and doors can generate a tiny bit of energy.</text>
									<tKey>HGS_Buildings_Archotech_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Mechanoid - Endgame structures. The floor, buildings and doors have two styles, vanilla and VFEM.</text>
									<tKey>HGS_Buildings_Mechanoid_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Mechanizer - A key resource processing workbench that can recycle just about anything, but has a tangible debuff to efficiency. Allows you to craft some resources. It also spawns mechaniteforge from time to time.</text>
									<tKey>HGS_Buildings_MechaniteCrafter_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Mechanites assembler - Mechanoid structure infinitely producing mechanites. Requires a mechanoid (or archotech) floor.</text>
									<tKey>HGS_Buildings_ComponentAssembler_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Gaurler - Has the features of the Gauranlen tree, but has faster spawn times and increased dryad pool (up to 6 dryads).\n\nRequirement: Ideology</text>
									<tKey>HGS_Buildings_Incubator_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Incubator - In a random period of time from 15 to 30 days, ELF (Cat without &lt;color=#a2c8ff&gt;kemomimilike&lt;/color&gt; mod) will spawn. Can only be built underground.\n\nRequirement: Vanilla Ideology Expanded - Dryads</text>
									<tKey>HGS_Buildings_Incubator_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Gloomlight - Similar to Vanilla Gloomlight, but gives beauty and illuminates with dark light.</text>
									<tKey>HGS_Buildings_Gloomlight_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Floor heater - Heats the room, does not require a power source, but you can not adjust the heating, it is always fixed at 17c.</text>
									<tKey>HGS_Buildings_Heater_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Stable power cell - Like the vanilla unstable power cell, but does not explode.</text>
									<tKey>HGS_Buildings_StablePowerCell_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Mending capsule - A triple bed for the &lt;color=#a2c8ff&gt;kemomimilike&lt;/color&gt; mod, but it can be used by all creatures with a body less than 0.51 (Only sentient ones). Significantly increases the rate of wound healing.</text>
									<tKey>HGS_Buildings_MendingStation_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Sleep pod - Single bed, parameters similar to Mending capsule.
									</text>
									<tKey>HGS_Buildings_SleepPod_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Mecha-hybridizer - Used to craft weapons and some other stuff.</text>
									<tKey>HGS_Buildings_AdvancedCrafter_Info_WVC</tKey>
								</li>
							</settings>
						</li>
						<!-- =============================================== -->
						<li Class="XmlExtensions.Setting.GapLine"/>
						<li Class="XmlExtensions.Setting.ScrollView">
							<height>64</height>
							<settings>
								<li Class="XmlExtensions.Setting.Text">
									<text>Universal conduit - Supports VE and SoS wiring.</text>
									<tKey>HGS_Buildings_UniversalConduit_Info_WVC</tKey>
								</li>
								<li Class="XmlExtensions.Setting.Checkbox">
									<key>WVC_Buildings_UniversalConduit_SupportPatch_VCHE</key>
									<label>VCHE</label>
									<defaultValue>True</defaultValue>
								</li>
								<li Class="XmlExtensions.Setting.Checkbox">
									<key>WVC_Buildings_UniversalConduit_SupportPatch_VFEP</key>
									<label>VFEP</label>
									<defaultValue>True</defaultValue>
								</li>
								<li Class="XmlExtensions.Setting.Checkbox">
									<key>WVC_Buildings_UniversalConduit_SupportPatch_SOS</key>
									<label>SOS2</label>
									<defaultValue>True</defaultValue>
								</li>
							</settings>
						</li>
					</li>
				</settings>
			</li>

			<!-- =================================================== -->

		</settings>
	</XmlExtensions.SettingsMenuDef>
</Defs>
