<Defs>

  <HediffDef Name="WVC_Hediff_BodyPart_NodeBlue" ParentName="AddedBodyPartBase" Abstract="True">
	<description>Improved and refined mechtech technology, the emphasis is on performance.</description>
	<scenarioCanAdd>false</scenarioCanAdd>
	<!-- 85caff -->
	<defaultLabelColor>(133,202,255)</defaultLabelColor>
	<priceOffset>1400</priceOffset>
	<addedPartProps>
	  <solid>true</solid>
	  <partEfficiency>1.45</partEfficiency>
	  <betterThanNatural>true</betterThanNatural>
	</addedPartProps>
  </HediffDef>

