<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <HediffDef Name="WVC_Hediff_BodyPart_Ancient" ParentName="AddedBodyPartBase" Abstract="True">
	<description>Advanced bionics from high-tech components. Surpasses cybertech bionics in quality, but has a drawback - weight.</description>
	<scenarioCanAdd>false</scenarioCanAdd>
	<defaultLabelColor>(251,255,251)</defaultLabelColor>
	<priceOffset>900</priceOffset>
	<addedPartProps>
	  <solid>true</solid>
	  <partEfficiency>1.35</partEfficiency>
	  <betterThanNatural>true</betterThanNatural>
	</addedPartProps>
  </HediffDef>

  <!--=====================================================================-->

  <HediffDef ParentName="WVC_Hediff_BodyPart_Ancient">
	<defName>WVC_AncientEye</defName>
	<label>ultratech eye</label>
	<labelNoun>an ultratech eye</labelNoun>
	<stages>
	  <li>
		<statOffsets>
			<Mass>1</Mass>
			<ArmorRating_Sharp>0.01</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.01</ArmorRating_Blunt>
			<ArmorRating_Heat>0.01</ArmorRating_Heat>
		</statOffsets>
	  </li>
	</stages>
	<tags>
		<li>Eye</li>
	</tags>
  </HediffDef>

